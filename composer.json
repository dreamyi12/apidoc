{
    "name": "dreamyi12/apidoc",
    "keywords": [
        "php",
        "hyperf"
    ],
    "authors": [
        {
            "name": "Fan Lee",
            "email": "dreamyi12@126.com",
            "homepage": "https://github.com/dreamyi12"
        }
    ],
    "license": "dream",
    "description": "A Powerful introduction swagger library for Hyperf.",
    "autoload": {
        "psr-4": {
            "Dreamyi12\\ApiDoc\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "tests"
        }
    },
    "require": {
        "php": ">=7.2",
        "ext-swoole": ">=4.4",
        "ext-json": "*",
        "hyperf/utils": "^1.1|^2.0",
        "hyperf/validation": "^1.1|^2.0",
        "kakuilan/php-helper": "^0.3.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "hyperf/testing": "1.1.*|^2.0",
        "hyperf/contract": "^1.1|^2.0",
        "hyperf/config": "1.1.*|^2.0",
        "hyperf/di": "1.1.*|^2.0",
        "hyperf/http-message": "^1.1|^2.0",
        "hyperf/http-server": "^1.1|^2.0",
        "nikic/fast-route": "^1.3",
        "phpstan/phpstan": "^0.10.5",
        "psr/container": "^1.0",
        "psr/http-message": "^1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit -c phpunit.xml --colors=always",
        "analyse": "phpstan analyse --memory-limit 1024M -l 0 src",
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
        "hyperf": {
            "config": "Dreamyi12\\ApiDoc\\ConfigProvider"
        }
    }
}
